# Compiled JavaScript files
# Note: index.js, logger.js, and validation.js are source code (not TypeScript compiled), so we track them
**/*.js
**/*.js.map
!index.js
!logger.js
!validation.js
!notifications/*.js
!tasks/*.js
!jest.config.js
!__tests__/**/*.js

# TypeScript v1 declaration files
typings/

# Environment files (contain secrets)
.env
.env.*
!.env.example

# Node.js dependency directory
node_modules/
